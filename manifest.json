{
  "name": "No Link",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "exclude_globs": [
        "*google*"
      ],
      "js": ["no_link.js"],
      "css": ["no_link.css"]
    }
  ],
  "icons": {
    "128": "icon.png",
    "48": "icon.png",
    "16": "icon.png"
  },
  "manifest_version": 2,
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "version": "0.1"
}
